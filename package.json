{
  "name": "secure-bridge-connect",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp public/_redirects dist/",
    "build:netlify": "vite build && cp public/_redirects dist/",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "analyze": "cross-env NODE_ENV=analyze vite build",
    "analyze:bundle": "cross-env ANALYZE_BUNDLE=true npm run analyze",
    "analyze:source-maps": "cross-env ANALYZE_SOURCE_MAPS=true npm run analyze",
    "analyze:duplicates": "cross-env ANALYZE_DUPLICATES=true npm run analyze",
    "analyze:unused": "cross-env ANALYZE_UNUSED=true npm run analyze",
    "analyze:circular-deps": "cross-env ANALYZE_CIRCULAR_DEPS=true npm run analyze",
    "analyze:runtime-deps": "cross-env ANALYZE_RUNTIME_DEPS=true npm run analyze",
    "analyze:node-modules": "cross-env ANALYZE_NODE_MODULES=true npm run analyze",
    "analyze:treemap": "cross-env ANALYZE_TREEMAP=true npm run analyze",
    "analyze:sunburst": "cross-env ANALYZE_SUNBURST=true npm run analyze",
    "analyze:network": "cross-env ANALYZE_NETWORK=true npm run analyze",
    "analyze:report": "cross-env ANALYZE_REPORT_HTML=true ANALYZE_REPORT_JSON=true ANALYZE_REPORT_TEXT=true npm run analyze"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@tanstack/react-query": "^5.24.1",
    "@tanstack/react-table": "^8.12.0",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "lucide-react": "^0.341.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-router-dom": "^6.22.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.3.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.10.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-bundle-analyzer": "^0.9.0",
    "vite-plugin-circular-dependency": "^0.2.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-plugin-pwa": "^0.19.0",
    "vite-plugin-radar": "^0.9.3",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vitest": "^1.3.1"
  }
}
